# SQL Server Connection Configuration for On-Premises Deployments
# Copy this file to .env and update with your actual connection details

# SQL Server instance (localhost, IP address, or server\instance)
SERVER_NAME=localhost
# SERVER_NAME=192.168.1.100
# SERVER_NAME=MYSERVER\SQLEXPRESS

# Database name
DATABASE_NAME=your-database-name

# SQL Server Authentication credentials
DB_USER=your-sql-username
DB_PASSWORD=your-sql-password

# Security and Connection Options
READONLY=false
TRUST_SERVER_CERTIFICATE=true
CONNECTION_TIMEOUT=30
